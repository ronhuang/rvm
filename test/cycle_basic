8d 4c 24 04              # lea ecx,[esp+4]
83 e4 f0                 # and esp,0xfffffff0
50                       # push eax
c1 f8 02                 # sar eax,0x02
34 2e                    # xor al,0x2e
89 e5                    # mov ebp,esp
45                       # inc ebp
83 ec 04                 # sub esp,0x04
80 7c 24 04 81           # cmp [esp+4],0x81
5d                       # pop ebp
